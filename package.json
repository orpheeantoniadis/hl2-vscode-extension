{
    "name": "hl2",
    "displayName": "hepiaLight2",
    "description": "hepiaLight2 for Visual Studio Code",
    "version": "0.6.3",
    "publisher": "orpheeantoniadis",
    "repository": "https://github.com/orpheeantoniadis/vscode-hl2.git",
    "icon": "resources/images/logo.png",
    "galleryBanner": {
        "color": "#046594",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.31.0"
    },
    "keywords": [
        "hepialight",
        "hepia",
        "micropython",
        "education",
        "iot"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./hl2",
    "contributes": {
        "commands": [
            {
                "command": "hl2.connect",
                "title": "hepialight : connect",
                "category": "hepiaLight2",
                "icon": {
                    "light": "./resources/icons/light/plug.svg",
                    "dark": "./resources/icons/dark/plug.svg"
                }
            },
            {
                "command": "hl2.run",
                "title": "hepialight : run file",
                "category": "hepiaLight2",
                "icon": {
                    "light": "./resources/icons/light/run.svg",
                    "dark": "./resources/icons/dark/run.svg"
                }
            },
            {
                "command": "hl2.update",
                "title": "hepialight : update",
                "category": "hepiaLight2",
                "icon": {
                    "light": "./resources/icons/light/update.svg",
                    "dark": "./resources/icons/dark/update.svg"
                }
            },
            {
                "command": "hl2.upload",
                "title": "hepialight : upload file",
                "category": "hepiaLight2",
                "icon": {
                    "light": "./resources/icons/light/upload.svg",
                    "dark": "./resources/icons/dark/upload.svg"
                }
            }
        ],
        "keybindings": [
            {
                "command": "hl2.run",
                "key": "ctrl+shift+r",
                "mac": "cmd+shift+r"
            },
            {
                "command": "hl2.upload",
                "key": "ctrl+shift+s",
                "mac": "cmd+shift+s"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.run",
                    "group": "navigation"
                },
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.upload",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.run",
                    "group": "navigation"
                },
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.upload",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.connect",
                    "group": "navigation"
                },
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.run",
                    "group": "navigation"
                },
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.update",
                    "group": "navigation"
                },
                {
                    "when": "resourceLangId == python",
                    "command": "hl2.upload",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "pwsh -nop -f ./scripts/mp-download.ps1 -copyonly",
        "download-native": "pwsh -nop -f ./scripts/mp-download.ps1 -ElectronVersions 7.1.1 -IgnoreNodeVersion",
        "build": "babel src --out-dir lib --source-maps",
        "build-dev": "babel src --out-dir lib --source-maps --watch",
        "package-prep": "npm install vsce -g",
        "package": "vsce package",
        "vscode:prepublish": "npm ci && npm prune"
    },
    "backup-required-to-include-binaries": {
        "postinstall": "pwsh -nop -f ./scripts/mp-download.ps1 -copyonly",
        "vscode:prepublish": "npm ci && npm prune"
    },
    "dependencies": {
        "binascii": "0.0.2",
        "bindings": "^1.5.0",
        "commander": "4.0.1",
        "copy-paste": "^1.3.0",
        "crc": "3.8.0",
        "crypto-js": "^3.1.9-1",
        "debug": "^4.1.1",
        "electron": "^6.1.2",
        "electron-rebuild": "^1.8.6",
        "element-resize-detector": "1.1.15",
        "glob": "^7.1.4",
        "lie": "^3.3.0",
        "nan": "2.14.0",
        "ncp": "^2.0.0",
        "node-abi": "^2.12.0",
        "object.assign": "^4.1.0",
        "ora": "^4.0.3",
        "promirepl": "^2.0.1",
        "prompt-list": "^3.2.0",
        "rxjs": "^6.5.3",
        "safe-buffer": "^5.2.0",
        "semver": "6.3.0",
        "serialport": "^8.0.5",
        "spdx-correct": "^3.1.0",
        "spdx-exceptions": "^2.2.0",
        "telnet-client": "^1.2.3",
        "typescript": "^3.7.2",
        "utf8": "^3.0.0",
        "xterm": "^4.2.0"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.11",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "6.24.1",
        "chai": "^4.2.0",
        "eslint": "^6.7.1",
        "mocha": "^6.2.2",
        "prebuild-install": "^5.3.3",
        "vscode": "^1.1.36"
    }
}
